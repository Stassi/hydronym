{
  "name": "@stassi/binary-transcoder",
  "version": "0.0.1",
  "description": "Encode and decode binary data. Supports multiple common encodings.",
  "keywords": [
    "8-bit",
    "binary",
    "decoder",
    "decoding",
    "encoder",
    "encoding",
    "hex",
    "hexadecimal",
    "ISO/IEC 8859-1",
    "Latin-1",
    "latin1",
    "octet",
    "transcoder",
    "transcoding",
    "uint8"
  ],
  "homepage": "https://github.com/Stassi/hydronym/tree/main/packages/binary-transcoder#readme",
  "license": "MIT",
  "author": "Andreas Stassivik <andreas@stassi.net>",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "repository": "github:Stassi/hydronym",
  "scripts": {
    "test": "jest"
  },
  "devDependencies": {
    "@jest/globals": "^29.2.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.0.3",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "os": [
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "root": true
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "preset": "ts-jest/presets/default-esm",
    "transform": {
      "<regex_match_files": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "prettier": {
    "endOfLine": "auto",
    "semi": false,
    "singleQuote": true
  }
}
